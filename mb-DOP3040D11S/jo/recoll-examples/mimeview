
# $machBld/jo/recoll-examples/mimeview
#  based of  /usr/share/recoll/examples/mimeview  which acts as the default
#  diff to   ~/.recoll/mimeview

xallexcepts = application/pdf application/postscript application/vnd.ms-visio.drawing application/x-dvi application/x-ipynb+json application/x-joplin-note text/html|chm text/html|epub text/html|gnuinfo text/html|rclman text/x-orgmode text/x-orgmode-sub

[view]
application/epub+zip = ebook-viewer %f
